                            ? e.removeEventListener("load", a)
                            : e.attachEvent && e.detachEvent("onload", a),
                        e.setAttribute("onload", null),
                        e.media = t
                    }
                    e.addEventListener
                        ? e.addEventListener("load", a)
                        : e.attachEvent && e.attachEvent("onload", a),
                    setTimeout(function () {
                        e.rel = "stylesheet",
                        e.media = "only x"
                    }),
                    setTimeout(a, 3e3)
                },
                o.poly = function () {
                    for (var e = n.document.getElementsByTagName("link"), t = 0; t < e.length; t++) {
                        var a = e[t];
